-- Players.q692q.PlayerGui.HilalWare.Lua.TuT Sürükle.Main.G_2.Araclar.Sekme1.Sekme.ScrollingFrame.TextLabel.Animasyon
local function C_5b()
	local script = LMG2L["Animasyon_5b"];
	local textLabel = script.Parent
	
	local tweenService = game:GetService("TweenService")
	
	local FINAL_POSITION = UDim2.new(0, 0, 0, 0)
	
	local INITIAL_POSITION = UDim2.new(0, -338, 0, 0)
	
	local tweenInfo = TweenInfo.new(0.5, Enum.EasingStyle.Quint, Enum.EasingDirection.Out, 0, false, 0.5)
	
	local function animateToFinalPosition()
	
	local tween = tweenService:Create(textLabel, tweenInfo, {Position = FINAL_POSITION})
	
	tween:Play()
	
	end
	
	textLabel.Position = INITIAL_POSITION
	
	animateToFinalPosition()	
end;
task.spawn(C_5b);
-- Players.q692q.PlayerGui.HilalWare.Lua.TuT Sürükle.Main.G_2.Araclar.Sekme2.Sekme.SpeedH.SpeedH
local function C_61()
	local script = LMG2L["SpeedH_61"];
	-- TP Walk Hız Kontrol Script
	
	local textBox = script.Parent
	
	local Players = game:GetService("Players")
	
	local RunService = game:GetService("RunService")
	
	local LocalPlayer = Players.LocalPlayer
	
	
	
	-- TP Walk değişkeni
	
	local tpWalkSpeed = 1 -- Varsayılan hız 1
	
	
	
	-- TextBox değiştiğinde hızı güncelle
	
	textBox:GetPropertyChangedSignal("Text"):Connect(function()
	
	    local text = textBox.Text
	
	    local speed = tonumber(text)
	
	    
	
	    if speed and speed > 0 and speed <= 500 then
	
	        tpWalkSpeed = speed
	
	        print("TP Walk hızı " .. speed .. " olarak ayarlandı!")
	
	    elseif text == "" then
	
	        tpWalkSpeed = 1 -- Varsayılan hıza dön (1)
	
	    else
	
	        warn("Geçersiz hız değeri! 1-500 arası sayı girin.")
	
	    end
	
	end)
	
	
	
	-- TP Walk sistemi
	
	local char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
	
	local hrp = char:WaitForChild("HumanoidRootPart")
	
	local humanoid = char:WaitForChild("Humanoid")
	
	
	
	RunService.Heartbeat:Connect(function(dt)
	
	    if not char or not char.Parent then
	
	        char = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
	
	        hrp = char:WaitForChild("HumanoidRootPart")
	
	        humanoid = char:WaitForChild("Humanoid")
	
	    end
	
	
	
	    local moveDir = humanoid.MoveDirection
	
	    if moveDir.Magnitude > 0 then
	
	        hrp.CFrame = hrp.CFrame + moveDir * tpWalkSpeed * dt
	
	    end
	
	end)
	
	
	
	print("TP Walk hız kontrolü aktif! Varsayılan hız: 1")	
end;
task.spawn(C_61);
-- Players.q692q.PlayerGui.HilalWare.Lua.TuT Sürükle.Main.G_2.Araclar.Sekme2.Sekme.Speed.LocalScript
local function C_63()
	local script = LMG2L["LocalScript_63"];
	-- WalkSpeed Ayarlama Script (Her değişiklikte)
	
	local textBox = script.Parent
	
	local Players = game:GetService("Players")
	
	local localPlayer = Players.LocalPlayer
	
	
	
	textBox:GetPropertyChangedSignal("Text"):Connect(function()
	
	    local text = textBox.Text
	
	    local speed = tonumber(text)
	
	    
	
	    if speed and speed > 0 and speed <= 9000000000000000000000000000000000000 then
	
	        if localPlayer.Character then
	
	            local humanoid = localPlayer.Character:FindFirstChildOfClass("Humanoid")
	
	            if humanoid then
	
	                humanoid.WalkSpeed = speed
	
	            end
	
	        end
	
	    end
	
	end)	
end;
task.spawn(C_63);
-- Players.q692q.PlayerGui.HilalWare.Lua.TuT Sürükle.Main.G_2.Araclar.Sekme6.Sekme.Listelemek
local function C_68()
	local script = LMG2L["Listelemek_68"];
	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	
	
	
	local listFrame = script.Parent:WaitForChild("List")
	
	local template = listFrame:WaitForChild("Template")
	
	
	
	-- Workspace içindeki Tool'lar için Handle bul
	
	local function getHandle(tool)
	
		for _, obj in ipairs(tool:GetDescendants()) do
	
			if obj.Name == "Handle" and obj:IsA("BasePart") then
	
				return obj
	
			end
	
		end
	
		return nil
	
	end
	
	
	
	-- Tool içindeki Script sayısı
	
	local function countScripts(tool)
	
		local count = 0
	
		for _, obj in ipairs(tool:GetDescendants()) do
	
			if obj:IsA("Script") or obj:IsA("LocalScript") then
	
				count += 1
	
			end
	
		end
	
		return count
	
	end
	
	
	
	-- Tool list item oluştur
	
	local function createItem(tool)
	
		local clone = template:Clone()
	
		clone.Visible = true
	
		clone.Parent = listFrame
	
	
	
		clone.ToolName.Text = tool.Name
	
		clone.Quantity.Text = tostring(countScripts(tool))
	
	
	
		-- EQUIP: Handle'ı oyuncunun pozisyonuna taşır
	
		clone.Equip.MouseButton1Click:Connect(function()
	
			local handle = getHandle(tool)
	
			local char = player.Character or player.CharacterAdded:Wait()
	
			local hrp = char:FindFirstChild("HumanoidRootPart")
	
			if handle and hrp then
	
				handle.CFrame = CFrame.new(hrp.Position) -- sadece handle ışınlanır
	
			end
	
		end)
	
	
	
		-- DELETE: Workspace içindeki Tool'u siler
	
		clone.Delete.MouseButton1Click:Connect(function()
	
			if tool and tool.Parent == workspace then
	
				tool:Destroy()
	
			end
	
			clone:Destroy()
	
		end)
	
	end
	
	
	
	-- Listeyi yenile, sadece Workspace içindeki Tool’ları al
	
	local function refresh()
	
		for _, child in ipairs(listFrame:GetChildren()) do
	
			if child:IsA("Frame") and child ~= template then
	
				child:Destroy()
	
			end
	
		end
	
	
	
		for _, obj in ipairs(workspace:GetDescendants()) do
	
			if obj:IsA("Tool") and obj.Parent == workspace then
	
				createItem(obj)
	
			end
	
		end
	
	end
	
	
	
	-- Sürekli yenileme
	
	task.spawn(function()
	
		while true do
	
			refresh()
	
			task.wait(0.5)
	
		end
	
	end)
	
	
end;
task.spawn(C_68);
-- Players.q692q.PlayerGui.HilalWare.Lua.TuT Sürükle.Main.G_2.Araclar.Sekme5.Sekme.Frame.AntiFling.AntiFling.click
local function C_7b()
	local script = LMG2L["click_7b"];
	local button = script.Parent
	
	-- Butonun bir üst dizininde (örneğin ScreenGui veya Ana Frame) "Background" adında bir Frame olduğunu varsayıyoruz.
	
	local background = button.Parent:WaitForChild("Background")
	
	-- Background Frame'inin içinde "HedefFrame" adında bir Frame olduğunu varsayıyoruz.
	
	local targetFrame = background:WaitForChild("HedefFrame") 
	
	
	
	-- Hedef Konumlar
	
	-- Açık Konum: {0, 22}, {0, 0}
	
	local OPEN_POSITION = UDim2.new(0, 22, 0, 0)
	
	-- Kapalı Konum (Başlangıç): {0, 0}, {0, 0}
	
	local CLOSED_POSITION = UDim2.new(0, 0, 0, 0)
	
	
	
	-- Durum takibi: Hedef Frame'in başlangıçta kapalı olduğunu varsayıyoruz.
	
	local isOpen = false 
	
	
	
	-- Butona tıklama olayı
	
	button.MouseButton1Click:Connect(function()
	
	    if isOpen == false then
	
	        -- Kapanış durumunda ise, aç
	
	        targetFrame.Position = OPEN_POSITION
	
	        isOpen = true
	
	    else
	
	        -- Açık durumda ise, kapat
	
	        targetFrame.Position = CLOSED_POSITION
	
	        isOpen = false
	
	    end
	
	end)	
end;
task.spawn(C_7b);
-- Players.q692q.PlayerGui.HilalWare.Lua.TuT Sürükle.Main.G_2.Araclar.Sekme5.Sekme.Frame.AntiFling.AntiFling.AAntiFFling
local function C_7c()
	local script = LMG2L["AAntiFFling_7c"];
	local button = script.Parent
	
	local Players = game:GetService("Players")
	
	local localPlayer = Players.LocalPlayer -- Kendi oyuncumuz
	
	local isPassable = false -- Geçirgenlik durumu. Başlangıçta KAPALI.
	
	
	
	-- Karakter parçalarına durumu uygulayan fonksiyon
	
	local function setCharacterPassable(character, shouldPass)
	
	    if not character then return end
	
	
	
	    local targetCanCollide = not shouldPass -- shouldPass=true ise CanCollide=false olur
	
	    
	
	    for _, part in ipairs(character:GetDescendants()) do
	
	        -- BasePart'ları (Aksesuar hariç) hedef al
	
	        if part:IsA("BasePart") and not part:IsA("Accessory") then
	
	            part.CanCollide = targetCanCollide
	
	        end
	
	    end
	
	end
	
	
	
	-- Oyuncunun karakteri yüklendiğinde durumu uygulayan fonksiyon
	
	local function onCharacterAdded(character, player)
	
	    -- SADECE yerel oyuncu DEĞİLSE uygula
	
	    if player ~= localPlayer then
	
	        -- Yüklendiği anda, mevcut isPassable durumunu uygula
	
	        setCharacterPassable(character, isPassable)
	
	    end
	
	end
	
	
	
	-- Tüm dış oyunculara durumu uygulayıp, genel durumu tersine çeviren ana fonksiyon
	
	local function applyPassableState()
	
	    -- 1. Durumu tersine çevir
	
	    isPassable = not isPassable
	
	    
	
	    -- 2. Mevcut oyunculara uygula (Kendi oyuncumuz hariç)
	
	    for _, player in ipairs(Players:GetPlayers()) do
	
	        if player ~= localPlayer then
	
	            if player.Character then
	
	                setCharacterPassable(player.Character, isPassable)
	
	            end
	
	        end
	
	    end
	
	    
	
	    print("Diğer oyuncuların geçirgenlik durumu şimdi: " .. (isPassable and "AÇIK" or "KAPALI"))
	
	end
	
	
	
	-- --- Başlangıç Kurulumu ---
	
	
	
	local function setupPlayerListeners(player)
	
	    -- Yeni oyuncu eklendiğinde, karakter yükleyiciyi bağla
	
	    player.CharacterAdded:Connect(function(character)
	
	        onCharacterAdded(character, player)
	
	    end)
	
	end
	
	
	
	-- Tüm mevcut ve yeni oyuncular için dinleyicileri kur
	
	for _, player in ipairs(Players:GetPlayers()) do
	
	    setupPlayerListeners(player)
	
	end
	
	Players.PlayerAdded:Connect(setupPlayerListeners)
	
	
	
	-- Butona tıklama olayını bağla
	
	button.MouseButton1Click:Connect(applyPassableState)	
end;
task.spawn(C_7c);
-- Players.q692q.PlayerGui.HilalWare.Lua.TuT Sürükle.Main.G_2.Araclar.Sekme5.Sekme.Frame.AntiFling.AntiFling.ses
