-- Players.q692q.PlayerGui.HilalWare.Lua.TuT Sürükle.Buttons.Kısalt.işlev
local function C_10()
	local script = LMG2L["işlev_10"];
	-- Bu LocalScript'i, Kısalt adlı TextButton'ın içine (veya Buton_2f script'inin içine) koyun.
	
	local button = script.Parent
	
	local tweenService = game:GetService("TweenService")
	
	
	
	-- ----------------------------------------------------------------------------------
	
	-- Hiyerarşi Yolu:
	
	-- Kısalt (script.Parent) -> Buttons -> TuT Sürükle -> Main -> G_2 (Hedef)
	
	-- ----------------------------------------------------------------------------------
	
	
	
	-- G_2 Frame'ini bulma (G2L["G_2_3b"]'yi scriptte direkt buluyoruz)
	
	-- Kısalt (Button) -> Buttons (Parent) -> TuT Sürükle (Parent) -> Main (Child) -> G_2 (Child)
	
	local tuTSurukle = button.Parent.Parent
	
	local mainFrame = tuTSurukle:WaitForChild("Main")
	
	local g2Frame = mainFrame:WaitForChild("G_2") -- Hareket edecek frame (G2L["G_2_3b"])
	
	
	
	-- Konum Bilgileri
	
	-- Butona basılmadan önceki hali (Kapalı/Başlangıç Konumu)
	
	local CLOSED_POSITION = UDim2.new(0, 0, 0, 0) 
	
	
	
	-- Butona basılınca gitmesi gereken hali (Açık/Hedef Konumu)
	
	local OPEN_POSITION = UDim2.new(0, 0, 0, -300) 
	
	
	
	-- Durum takibi için değişken (G_2'nin açık/kapalı durumunu tutar)
	
	local isOpen = false
	
	
	
	-- Tween (Animasyon) Bilgileri
	
	local tweenInfo = TweenInfo.new(
	
	    0.3, -- Animasyon süresi (0.3 saniye, daha hızlı)
	
	    Enum.EasingStyle.Quart, -- Yumuşak ve hızlı bir geçiş
	
	    Enum.EasingDirection.Out,
	
	    0, -- Tekrar yok
	
	    false, -- Tersine döndürme yok
	
	    0 -- Gecikme yok
	
	)
	
	
	
	-- ----------------------------------------------------
	
	-- İŞLEV: G_2 Frame'ini Hareket Ettir (Animasyon)
	
	-- ----------------------------------------------------
	
	local function animateG2()
	
	    local targetPosition
	
	    
	
	    -- Konumu kontrol et, hareket hedefini belirle
	
	    if isOpen == false then
	
	        -- Şu an kapalı, açma pozisyonuna git
	
	        targetPosition = OPEN_POSITION
	
	        isOpen = true
	
	    else
	
	        -- Şu an açık, kapatma pozisyonuna git
	
	        targetPosition = CLOSED_POSITION
	
	        isOpen = false
	
	    end
	
	    
	
	    -- Tween objesini oluştur ve çalıştır
	
	    local tween = tweenService:Create(g2Frame, tweenInfo, {Position = targetPosition})
	
	    tween:Play()
	
	end
	
	
	
	-- ----------------------------------------------------
	
	-- BUTON TIKLAMA OLAYI
	
	-- ----------------------------------------------------
	
	button.MouseButton1Click:Connect(animateG2)
	
	
	
	-- Başlangıçta konumu ayarla (Emin olmak için)
	
	g2Frame.Position = CLOSED_POSITION	
end;
task.spawn(C_10);
-- Players.q692q.PlayerGui.HilalWare.Lua.TuT Sürükle.Buttons.Kısalt.Ses
